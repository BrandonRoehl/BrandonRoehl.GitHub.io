<!-- RSS generated by Radio UserLand v8.0.5 on 9/30/2002; 4:00:00 AM Pacific -->
<rss version="2.0" xmlns:blogChannel="http://brandonroehl.org/#/blog">
    <channel>
        <title>Brandon Roehl's Blog</title>
        <link>http://brandonroehl.org</link>
        <description>A weblog about scripting and stuff like that.</description>
        <language>en-us</language>
        <copyright>Copyright Brandon Roehl</copyright>
        <lastBuildDate>{{ site.time }}</lastBuildDate>
        <docs></docs>
        <generator>Jekyll</generator>
        <managingEditor>me@brandonroehl.org</managingEditor>
        <webMaster>me@brandonroehl.org</webMaster>
        <ttl>1440</ttl>
        <!-- <ttl>60</ttl> -->
        <!-- <blogChannel:blogRoll>http://radio.weblogs.com/0001015/userland/scriptingNewsLeftLinks.opml</blogChannel:blogRoll> -->
        <!-- <blogChannel:mySubscriptions>http://radio.weblogs.com/0001015/gems/mySubscriptions.opml</blogChannel:mySubscriptions> -->
        <!-- <blogChannel:blink>http://diveintomark.org/</blogChannel:blink> -->
        <!-- <category domain="Syndic8">1765</category> -->
        {%- for post in site.posts -%}
        {% assign name = post.url|remove:'/blog/'|remove:'.html' %}
        <item>
            <title>{{ post.title }}</title>
            {%- for category in post.categories -%}
            <category>{{ category }}</category>
            {%- endfor -%}
            <category>{{ post.category }}</category>
            <description>{{ post.excerpt }}</description>
            <pubDate>{{ post.date | date: "%b %d, %Y" }}</pubDate>
            <link>http://brandonroehl.org/#/blog/{{name}}</link>
            <guid>{{ post.id }}</guid>
        </item>
        {%- endfor %}
    </channel>
</rss>
